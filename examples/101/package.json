{
  "name": "zksync-101",
  "version": "1.0.0",
  "description": "A project to learn how to build with zksync-cli",
  "private": true,
  "author": "Matter Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matter-labs/zksync-contract-templates.git"
  },
  "keywords": [
    "zksync",
    "zksync-cli",
    "hardhat"
  ],
  "scripts": {
    "beacon-test": "hardhat test test/3-proxy-contracts/BeaconProxy.test.ts --network hardhat",
    "beacon-deploy": "hardhat run ./deploy/3-proxy-contracts/beacon/deploy.ts --network anvilZKsync",
    "beacon-upgrade": "hardhat run ./deploy/3-proxy-contracts/beacon/upgrade.ts --network anvilZKsync",
    "transparent-deploy": "hardhat run ./deploy/3-proxy-contracts/transparent/deploy.ts --network anvilZKsync",
    "transparent-upgrade": "hardhat run ./deploy/3-proxy-contracts/transparent/upgrade.ts --network anvilZKsync",
    "uups-deploy": "hardhat run ./deploy/3-proxy-contracts/uups/deploy.ts --network anvilZKsync",
    "uups-upgrade": "hardhat run ./deploy/3-proxy-contracts/uups/upgrade.ts --network anvilZKsync",
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "test": "hardhat test --network hardhat"
  },
  "bugs": {
    "url": "https://github.com/matter-labs/zksync-contract-templates/issues"
  },
  "homepage": "https://github.com/matter-labs/zksync-contract-templates#readme",
  "devDependencies": {
    "@matterlabs/hardhat-zksync": "^1.6.1",
    "@matterlabs/zksync-contracts": "1.0.0-alpha.9",
